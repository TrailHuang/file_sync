<!doctype html>
<html lang="ch">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <meta name="format-detection" content="telephone=no">
    <title>文件同步客户端配置</title>
    <script src="javascripts/jquery.min.js"></script>
    <script src="javascripts/bootstrap.min.js"></script>
    <script src="javascripts/setvalue.js"></script>
    <script>
        var default_cfg = JSON.parse('<%- config %>');
        $(function() {
            console.log(default_cfg);
            $(".meun-item").click(function() {
                $(".meun-item").removeClass("meun-item-active");
                $(this).addClass("meun-item-active");
                var itmeObj = $(".meun-item").find("img");
                itmeObj.each(function() {
                    var items = $(this).attr("src");
                    items = items.replace("_grey.png", ".png");
                    items = items.replace(".png", "_grey.png")
                    $(this).attr("src", items);
                });
                var attrObj = $(this).find("img").attr("src");;
                attrObj = attrObj.replace("_grey.png", ".png");
                $(this).find("img").attr("src", attrObj);
            });
            $("#topAD").click(function() {
                $("#topA").toggleClass(" glyphicon-triangle-right");
                $("#topA").toggleClass(" glyphicon-triangle-bottom");
            });
            $("#topBD").click(function() {
                $("#topB").toggleClass(" glyphicon-triangle-right");
                $("#topB").toggleClass(" glyphicon-triangle-bottom");
            });
            $("#topCD").click(function() {
                $("#topC").toggleClass(" glyphicon-triangle-right");
                $("#topC").toggleClass(" glyphicon-triangle-bottom");
            });
            $("#topDD").click(function() {
                $("#topD").toggleClass(" glyphicon-triangle-right");
                $("#topD").toggleClass(" glyphicon-triangle-bottom");
            });

            $("#topXZ").click(function() {
                $("#topX").toggleClass(" glyphicon-triangle-right");
                $("#topX").toggleClass(" glyphicon-triangle-bottom");
            });

            $("#topYZ").click(function() {
                $("#topY").toggleClass(" glyphicon-triangle-right");
                $("#topY").toggleClass(" glyphicon-triangle-bottom");
            });
            $("#topZZ").click(function() {
                $("#topZ").toggleClass(" glyphicon-triangle-right");
                $("#topZ").toggleClass(" glyphicon-triangle-bottom");
            });

            $("#topWZ").click(function() {
                $("#topW").toggleClass(" glyphicon-triangle-right");
                $("#topW").toggleClass(" glyphicon-triangle-bottom");
            });

            $(".toggle-btn").click(function() {
                $("#leftMeun").toggleClass("show");
                $("#rightContent").toggleClass("pd0px");
            })

            set_html_value(default_cfg);
            log_forward();

            $('#reviseTask').on('show.bs.modal', function(event) {
                var a = $(event.relatedTarget) // relatedTarget 事件属性返回与事件的目标节点相关的节点。
                var taskid = a.context.dataset.taskid;
                set_modify_task(default_cfg.tasklist, taskid);
            });
            $('#deleteTask').on('show.bs.modal', function(event) {
                var a = $(event.relatedTarget) // relatedTarget 事件属性返回与事件的目标节点相关的节点。
                var taskid = a.context.dataset.taskid;
                $('#deleteTask button[name="save"]').attr("data-taskid", taskid);
                //set_delete_task(default_cfg.tasklist, taskid);
            });

            $('#add_backup').hide();
            $('#addTask select[name="work_direction"]').change(function() {
                if ($(this).val() == "sync") {
                    $('#add_recv_t').show();
                    $('#add_send_t').show();
                } else if ($(this).val() == "recv") {
                    $('#add_recv_t').show();
                    $('#add_send_t').hide();
                } else {
                    $('#add_recv_t').hide();
                    $('#add_send_t').show();
                }
            });
            $('#reviseTask select[name="work_direction"]').change(function() {
                if ($(this).val() == "sync") {
                    $('#mod_recv_t').show();
                    $('#mod_send_t').show();
                } else if ($(this).val() == "recv") {
                    $('#mod_recv_t').show();
                    $('#mod_send_t').hide();
                } else {
                    $('#mod_recv_t').hide();
                    $('#mod_send_t').show();
                }
            })
            $('select[name="workmode"]').change(function() {

                if ($(this).val() == "move") {
                    $('#add_backup').show();
                } else {
                    $('#add_backup').hide();
                }

            })
            set_sub_div();

        })
        
        function set_sub_div()
        {
        	jQuery.each(default_cfg, function(key, val) {
        		if (key != "name_black" && key != "name_white") {
        			var pidv = $("#file_conf_" + key);
        			var childdiv=$('<div></div>');
	        		childdiv.addClass('rowinfo');
        			$.each(val, function(subkey, subval) {   
	        			childdiv.append(subkey + "&nbsp;&nbsp;:&nbsp;&nbsp;"+ subval + "<br\>")
        			});
        			
	        		childdiv.appendTo(pidv);
        		}
        		else
        		{
        			var pidv = $("#file_conf_name");
        			var childdiv=$('<div></div>');
	        		childdiv.addClass('rowinfo');
	        		childdiv.append(key + ":<br\>");
        			$.each(val, function(subkey, subval) { 
	        			
	        			//$.each(subval, function(i, tmp) {
	        				childdiv.append("&nbsp;&nbsp;" + subkey + "<br\>");
	        			//});
	        			childdiv.append()
        			});
        			childdiv.appendTo(pidv);        			
        		}
        	});
        	 
                    
        	
        }

        function submit_add_task() {
            var form_data = $('#form_addtask').serialize();
            $.ajax({
                url: "action/add_task",
                data: form_data,
                type: "post",
                success: function(data) {
                    $('#addTask').modal('hide');
                    taskobj = JSON.parse(data);
                    default_cfg = taskobj;
                    set_tasklist(default_cfg.tasklist);
                },
                error: function() {
                    alert('添加任务失败!');
                }
            });

            return false;
        }

        function submit_modify_task() {
            var form_data = $('#form_modifytask').serialize();
            $.ajax({
                url: "action/modify_task",
                data: form_data,
                type: "post",
                success: function(data) {
                    $('#reviseTask').modal('hide');
                    taskobj = JSON.parse(data);
                    default_cfg = taskobj;
                    set_tasklist(default_cfg.tasklist);

                },
                error: function() {
                    alert('修改任务失败!');
                }
            });

            return false;
        }

        function submit_delete_task(taskid) {
            $.ajax({
                url: "action/delete_task",
                data: {
                    "taskid": taskid
                },
                type: "post",
                success: function(data) {
                    $('#deleteTask').modal('hide');
                    delete_task(taskid, default_cfg.tasklist);
                    var divid = "taskdiv" + taskid;
                    $("#" + divid).remove();
				
                },
                error: function() {
                    alert('删除任务失败!');
                }
            });

            return false;
        }
        
        function restart_server()
		{
			
			$.ajax({
                url: "action/restart",
                type: "post",
                success: function(data) {
                    alert('重启成功!');		
                },
                error: function(data,type, err) {
                    alert(err);
                }
            });

            return false;
		}

        function log_forward() {
            var show = "";
            show = $("input[name='forward']:checked").val();
            if (show == 'yes') {
                $('#forward_addr').show();
                $('#forward_port').show();
            } else {
                $('#forward_addr').hide();
                $('#forward_port').hide();
            }
        }

        function cloud_scan() {
            var show = "";
            show = $("input[name='virus_scan']:checked").val();
            if (show == "yes") {
                $('#cloud_scan').show();
            } else {
                $('#cloud_scan').hide();
            }
        }
    </script>
    <script src="javascripts/action.js"></script>
