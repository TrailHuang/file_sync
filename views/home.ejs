                                                    <div class="form-inline col-xs-8">
                                                        <input type="text" class="form-control input-sm duiqi input-mini" name="worktime" value="00:00:00-23:59:59" placeholder="">
                                                    </div>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn btn-xs btn-white" data-dismiss="modal">取消</button>
                                    <button type="button" class="btn btn-xs btn-green" onclick="submit_add_task()">保存</button>
                                </div>
                            </div>
                            <!-- /.modal-content -->
                        </div>
                        <!-- /.modal-dialog -->
                    </div>
                    <!-- /.modal -->

                    <!--弹出修改任务窗口-->
                    <div class="modal fade" id="reviseTask" role="dialog" aria-labelledby="gridSystemModalLabel">
                        <div class="modal-dialog" role="document">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
																					aria-hidden="true">&times;</span></button>
                                    <h4 class="modal-title" id="gridSystemModalLabel">修改用户</h4>
                                </div>
                                <div class="modal-body">
                                    <div class="container-fluid">
                                        <form id="form_modifytask" class="form-horizontal">
                                            <div class="form-group">
                                                <label for="sName" class="col-xs-3 control-label">任务号:</label>
                                                <div class="col-xs-8">
                                                    <input type="email" class="form-control input-sm duiqi" name="taskid" placeholder="">
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label for="sName" class="col-xs-3 control-label">传输方向:</label>
                                                <div class="col-xs-8">
                                                    <select name="work_direction" class="form-control select-duiqi">
                                                        <option value="sync">双向传输</option>
                                                        <option value="send">单向发送</option>
                                                        <option value="recv">单向接收</option>
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label for="sLink" class="col-xs-3 control-label">工作目录:</label>
                                                <div class="col-xs-8">
                                                    <input type="" class="form-control input-sm duiqi" name="path" placeholder="">
                                                </div>
                                            </div>
                                            <div id="mod_recv_t">
                                                <div class="form-group">
                                                    <label for="situation" class="col-xs-3 control-label">是否覆盖:</label>
                                                    <div class="col-xs-8">
                                                        <label class="control-label" for="anniu">
                                                            <input type="radio" name="iscover" value="yes" checked="true">覆盖
                                                        </label> &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                                        <label class="control-label" for="meun">
                                                            <input type="radio" name="iscover" value="no">不覆盖
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                            <div id="mod_send_t">
                                                <div class="form-group">
                                                    <label for="sOrd" class="col-xs-3 control-label">工作模式:</label>
                                                    <div class="col-xs-8">
                                                        <select class="form-control select-duiqi" name="workmode">
                                                            <option value="append">增量更新</option>
                                                            <option value="identical">完全一致</option>
                                                            <option value="delete">源端删除</option>
                                                            <option value="move">源端移动</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="form-group" id="mod_backup">
                                                    <label for="sKnot" class="col-xs-3 control-label">移动目录:</label>
                                                    <div class="col-xs-8">
                                                        <input type="" class="form-control input-sm duiqi" name='backup' placeholder="" value="backup">
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label for="sKnot" class="col-xs-3 control-label">监控策略:</label>
                                                    <div class="col-xs-8">
                                                        <select class="form-control select-duiqi" name="monitor">
															<option value="scanner">扫描模式</option>
                                                            <option value="notify">触发模式</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label for="situation" class="col-xs-3 control-label">运行:</label>
                                                    <div class="col-xs-8">
                                                        <label class="control-label" for="anniu">
                                                            <input type="radio" name="run" value="yes" checked="true">运行
                                                        </label> &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                                        <label class="control-label" for="meun">
                                                            <input type="radio" name="run" value="no">不运行
                                                        </label>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label for="situation" class="col-xs-3 control-label">首次扫描:</label>
                                                    <div class="col-xs-8">
                                                        <label class="control-label" for="anniu">
                                                            <input type="radio" name="first" value="yes" checked="true">开启
                                                        </label> &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                                        <label class="control-label" for="meun">
                                                            <input type="radio" name="first" value="no">关闭
                                                        </label>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label for="situation" class="col-xs-3 control-label">子目录:</label>
                                                    <div class="col-xs-8">
                                                        <label class="control-label" for="anniu">
                                                            <input type="radio" name="subdir" value="yes" checked="true">同步
                                                        </label> &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                                        <label class="control-label" for="meun">
                                                            <input type="radio" name="subdir" value="no">不同步
                                                        </label>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label for="sOrd" class="col-xs-3 control-label">加密方式:</label>
                                                    <div class="col-xs-8">
                                                        <select class="form-control select-duiqi" name="encrypt">
															<option value="des">des</option>
															<option value="3des">3des</option>
															<option value="aes">aes</option>
															<option value="rc4">rc4</option>
															<option value="no">不加密</option>
														</select>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label for="sLink" class="col-xs-3 control-label">工作时间:</label>
                                                    <div class="form-inline col-xs-8">
                                                        <input type="text" class="form-control input-sm duiqi input-mini" name="worktime" value="00:00:00-23:59:59" placeholder="">
                                                    </div>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn btn-xs btn-white" data-dismiss="modal">取消</button>
                                    <button type="button" class="btn btn-xs btn-green" onclick="submit_modify_task()">保存</button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!--弹出修改任务窗口-->

                    <!--弹出删除任务警告窗口-->
                    <div class="modal fade" id="deleteTask" role="dialog" aria-labelledby="gridSystemModalLabel">
                        <div class="modal-dialog" role="document">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                        <span aria-hidden="true">&times;</span>
                                    </button>
                                    <h4 class="modal-title" id="gridSystemModalLabel">提示</h4>
                                </div>
                                <div class="modal-body">
                                    <div class="container-fluid">
                                        确定要删除该用户？删除后不可恢复！
                                    </div>
                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn btn-xs btn-white" data-dismiss="modal">取消</button>
                                    <button type="button" class="btn btn-xs btn-danger" name="save" onclick="submit_delete_task(this.dataset.taskid)">保存</button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!--弹出删除任务警告窗口-->

                </div>
                <div role="tabpanel" class="tab-pane" id="chan">
                    <div class="check-div form-inline">
                        <div class="col-xs-3">
                            <button class="btn btn-yellow btn-xs" data-toggle="modal" onclick="restart_server()">重启服务 </button>
                        </div>
					</div>
				</div>
            </div>
        </div>
    </div>

</body>

</html